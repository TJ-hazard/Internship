<div class="grid-container">
  <div class="card span2">
    <div class="card-title">Students</div>
    <div class="card-number"><%= totals.student %></div>
  </div>
  <div class="card span2">
    <div class="card-title">Institutions</div>
    <div class="card-number"><%= totals.institution %></div>
  </div>
  <div class="card span2">
    <div class="card-title">Industries</div>
    <div class="card-number"><%= totals.industries %></div>
  </div>
  <div class="card offsetBy1">
    <div class="card-title">Supervisors</div>
    <div class="card-number"><%= totals.institution_supervisor %></div>
  </div>
  <div class="card span2">
    <div class="card-title">Industry Supervisors</div>
    <div class="card-number"><%= totals.industry_supervisor %></div>
  </div>
</div>

<div class="pie-chart"></div>
